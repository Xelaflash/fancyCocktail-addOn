{
  "name": "Fancy cocktail",
  "version": "1.0",
  "description": "Get a fancy cocktail recipe on new tab",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["dist/bundle.js"],
    "run_at": "document_start"
  }],
  "icons": {
    "16": "src/assets/images/icon.png",
    "32": "src/assets/images/icon.png",
    "48": "src/assets/images/icon.png",
    "128": "src/assets/images/icon.png"
  }
}
