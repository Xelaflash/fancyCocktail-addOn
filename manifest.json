{
  "name": "Fancy cocktail",
  "version": "1.0",
  "description": "Get a fancy cocktail recipe on new tab",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["dist/bundle.js"],
    "run_at": "document_start"
  }]
}
